<% if @submissions.where(status: status).size > 0 %>
  <table class="table">
  <thead>
    <tr>
      <th>Type</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <% @submissions.where(status: status).page(params["#{status}_page"].to_i).per(10).each do |submission| %>
      <tr>
        <td>
          <%= submission.model_type.titleize %>
        </td>
        <td class="value-cell">
          <%= submission.value %>
        </td>
      </tr>
    <% end %>
  </tbody>
  </table>
  <%= paginate(@submissions.where(status: status).page(params["#{status}_page"].to_i).per(10), param_name: "#{status}_page", window: 1) %>
<% end %>